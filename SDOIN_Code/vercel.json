{
  "version": 2,
  "builds": [
      {
          "src": "package.json",
          "use": "vercel-sapper"
      },
      {
          "src": "./server.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/SDOIN_Code/routes/index.js",
          "dest": "./server.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
              "Access-Control-Allow-Origin": "*"
          }
      }
  ]
}